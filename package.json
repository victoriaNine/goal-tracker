{
  "name": "goal-tracker",
  "version": "2.0.0",
  "private": true,
  "description": "Demo app used in Delicious Insightsâ€™ JS Total training course",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js --progress",
    "deploy": "surge --project public",
    "doc": "rimraf doc && groc",
    "lint": "eslint .",
    "start": "node tools/dev-server.js",
    "start:d": "webpack-dashboard -- npm start",
    "test": "jest",
    "test:cov": "npm test -- --coverage",
    "test:watch": "npm test -- --watch"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions",
              "> 1%",
              "safari >= 10"
            ],
            "node": "current"
          }
        }
      ],
      "react",
      "stage-3"
    ],
    "plugins": [
      "transform-class-properties",
      "transform-decorators-legacy"
    ],
    "env": {
      "development": {
        "presets": [
          "react-hmre"
        ]
      }
    }
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "plugins": [
      "standard",
      "import"
    ],
    "extends": [
      "standard",
      "standard-jsx",
      "plugin:import/errors"
    ],
    "rules": {
      "import/newline-after-import": "error",
      "import/no-extraneous-dependencies": "error",
      "import/no-named-as-default-member": "warn"
    },
    "env": {
      "browser": true,
      "node": true,
      "jest": true
    }
  },
  "keywords": [
    "node",
    "nodejs",
    "training",
    "goals",
    "tracker",
    "demo"
  ],
  "author": "Christophe Porteneuve <christophe@delicious-insights.com>",
  "license": "No-License (see LICENSE file)",
  "dependencies": {
    "airbnb-prop-types": "^2.5.2",
    "autobind-decorator": "^1.3.3",
    "history": "^4.6.0",
    "isomorphic-fetch": "^2.2.1",
    "material-ui": "^0.17.0",
    "moment": "^2.11.2",
    "pouchdb": "^6.1.2",
    "react": "^15.4.2",
    "react-document-title": "^2.0.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "react-tap-event-plugin": "^2.0.0",
    "reduce-reducers": "^0.1.2",
    "redux": "^3.5.2",
    "redux-pouchdb": "^0.1.1",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "autoprefix": "^1.0.1",
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.0",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-hmre": "^1.1.0",
    "babel-preset-stage-3": "^6.22.0",
    "body-parser": "^1.17.1",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.6.1",
    "colors": "^1.1.2",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.0",
    "dirty-chai": "^1.2.2",
    "enzyme": "^2.7.1",
    "enzyme-to-json": "^1.5.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^7.0.1",
    "eslint-config-standard-jsx": "^3.3.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.10.0",
    "eslint-plugin-standard": "^2.1.1",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "groc": "^0.8.0",
    "istanbul": "^0.4.3",
    "jest": "^19.0.2",
    "jest-matchers": "^19.0.0",
    "marked": "^0.3.6",
    "morgan": "^1.8.1",
    "offline-plugin": "^4.6.1",
    "postcss-loader": "^1.3.3",
    "react-addons-test-utils": "^15.4.2",
    "react-test-renderer": "^15.4.2",
    "rimraf": "^2.6.1",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.9.0",
    "style-loader": "^0.16.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "surge": "^0.19.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.3.3",
    "webpack-dashboard": "^0.3.0",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.18.0"
  }
}
